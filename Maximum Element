// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int main() 
{
    int n,i;
    cin>>n;
    int a[n];
    
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    int max=a[0];
    
    for(i=0;i<n;i++)
    {
        if(max<a[i])
        {
            max=a[i];
        }
    }
    cout<<"Max number is :"<<max;

    return 0;
}
